
public class Recursion {

	public int factorial(int a) {
		if(a==1)
			return  1;
		return a*factorial(a-1);
	}
	public static void main(String[] args) {
		Recursion ob=new Recursion();
		System.out.println(ob.factorial(5));
		String s1="Welcome";
		s1.concat("Bala");
		System.out.println(s1);
	}
}
