

abstract class Tub{
	Tub(){
		System.out.println("Hiiiiiiiii");
	}
}
public class ImplementingClass extends Tub implements A,B{
public static void main(String[] args) {
	A b=new ImplementingClass();
	b.show();
	B k=new ImplementingClass();
	k.show();
}

@Override
public void show() {
	// TODO Auto-generated method stub
	
}
}
